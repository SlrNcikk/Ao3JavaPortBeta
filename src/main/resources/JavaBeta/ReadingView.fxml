<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?> <VBox prefHeight="700.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaBeta.ReadingController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>
        <HBox alignment="CENTER_LEFT" spacing="10.0">
            <children>
                <Button fx:id="downloadButton" onAction="#onDownloadButtonClick" text="Download Story" />
                <Region HBox.hgrow="ALWAYS" />
                <ChoiceBox fx:id="themeChoiceBox" prefWidth="150.0" />
            </children>
        </HBox>
        <WebView fx:id="storyWebView" VBox.vgrow="ALWAYS" />
    </children>
</VBox>